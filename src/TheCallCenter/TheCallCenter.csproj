    <AspNetCoreHostingModel>InProcess</AspNetCoreHostingModel>
    <LangVersion>10.0</LangVersion>
    <Nullable>enable</Nullable>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
